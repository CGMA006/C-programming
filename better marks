#include <stdio.h>
int main() {
    int marks;

    printf("Enter your marks: ");
    scanf("%d", &marks);

    if (marks >= 40 && marks < 101) {
        printf("You pass\n");
    } if (marks <= 40){
        printf("You fail\n");
    }
    if ("marks > 100"){
        printf ("Invalid");
    }

    return 0;
}
