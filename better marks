#include <stdio.h>
int main() {
    int marks;

    printf("Enter your marks: ");
    scanf("%d", &marks);

    if (marks >= 0 && marks <= 100) {
        if (marks >= 40) {
            printf("You pass\n");
        } else {
            printf("You fail\n");
        }
    } else {
        printf("Invalid input\n");
    }

    return 0;
}
