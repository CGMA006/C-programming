#include <stdio.h>
int main() {
    int a = 10, b = 20;
    if (a > 5) {
        if (b > 15) {
            printf("a is greater than 5 and b is greater than 15");
        } else {
            printf("a is greater than 5 but b is not greater than 15");
        }
    } else {
        printf("a is not greater than 5");
    }
    return 0;
}
