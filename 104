#include <stdio.h>
int main() {
    char ch = 'B';
    switch (ch) {
        case 'A':
            printf("Grade A");
            break;
        case 'B':
            printf("Grade B");
            break;
        case 'C':
            printf("Grade C");
            break;
        default:
            printf("Invalid grade");
    }
    return 0;
}
