#include <stdio.h>
int main() {
    int x = 2, y = 3;
    
    switch (x) {
        case 1:
            printf("x is 1\n");
            break;
        case 2:
            switch (y) {
                case 1:
                    printf("x is 2 and y is 1\n");
                    break;
                case 3:
                    printf("x is 2 and y is 3\n");
                    break;
            }
            break;
        default:
            printf("x is not 1 or 2\n");
    }
    
    return 0;
}
